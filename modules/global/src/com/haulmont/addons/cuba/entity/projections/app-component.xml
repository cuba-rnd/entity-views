<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<app-component xmlns="http://schemas.haulmont.com/cuba/app-component.xsd"
               dependsOn="com.haulmont.cuba">
    <module blocks="*"
            name="global">
        <artifact appJar="true"
                  name="entity-projections-global"/>
        <property name="description"
                  value="Entity Projections"/>
    </module>
    <module blocks="core"
            dependsOn="global"
            name="core">
        <artifact appJar="true"
                  name="entity-projections-core"/>
        <property name="cuba.springContextConfig"
                  value="+com/haulmont/addons/cuba/entity/projections/spring.xml"/>
    </module>
    <module name="web"
            dependsOn="global"
            blocks="web">
        <artifact name="entity-projections-web"
                  appJar="true"/>
        <property name="cuba.springContextConfig"
                  value="+com/haulmont/addons/cuba/entity/projections/web-spring.xml"/>
    </module>
</app-component>
